{
  "name": "Triptify",
  "version": "0",
  "description": "Trips No Problem",


  "manifest_version": 2,
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png",
    "default_popup": "background.html"
  },
  "content_scripts": [ {
    "js": [ "jquery-2.0.2.min.js", "jquery.livequery.min.js", "bootstrap.min.js", "background.js" ],
    "css": ["customStyles.css", "bootstrap.css", "bootstrap-theme.css"],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "web_accessible_resources" : [ "jquery-2.0.2.min.js", "jquery.livequery.min.js", "bootstrap.min.js", "background.js" ],
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}